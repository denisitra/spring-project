{"env": {
  "browser": true,
  "es6": true
},
  "extends": [
    "eslint:recommended",
    "react-app",
    "prettier"
  ],
  "rules": {
    "no-var": "error",
    "semi": "error",
    "no-multi-spaces": "error",
    "space-in-parens": "error",
    "no-multiple-empty-lines": "error",
    "prefer-const": "error",
    "no-use-before-define": "error"
  },
  "plugins": [
    "prettier"
  ]
}
